{
  "name": "@japa/snapshot",
  "description": "Snapshot testing plugin for Japa",
  "version": "2.0.0-0",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "index.ts",
    "src",
    "build/src",
    "build/index.d.ts",
    "build/index.js",
    "build/index.d.ts.map"
  ],
  "exports": {
    ".": "./build/index.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "release": "np --message=\"chore(release): %s\"",
    "version": "npm run build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels .github/labels.json japa/snapshot",
    "quick:test": "node --loader ts-node/esm bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.1.5",
    "@adonisjs/prettier-config": "^1.1.5",
    "@adonisjs/tsconfig": "^1.1.5",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@japa/assert": "^1.4.1",
    "@japa/core": "^7.3.3",
    "@japa/expect": "^2.0.2",
    "@japa/file-system": "^2.0.0-0",
    "@japa/run-failed-tests": "^1.1.1",
    "@japa/runner": "^2.5.1",
    "@japa/spec-reporter": "^1.3.3",
    "@poppinss/dev-utils": "^2.0.3",
    "@swc/core": "^1.3.66",
    "@types/dedent": "^0.7.0",
    "@types/node": "^20.3.1",
    "@types/stack-utils": "^2.0.1",
    "c8": "^8.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.43.0",
    "expect": "^29.5.0",
    "github-label-sync": "^2.3.1",
    "got": "^13.0.0",
    "husky": "^8.0.3",
    "np": "^8.0.4",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "dedent": "^0.7.0",
    "jest-message-util": "^29.5.0",
    "magic-string": "^0.30.0",
    "pretty-format": "^29.5.0",
    "stack-utils": "^2.0.6"
  },
  "peerDependencies": {
    "@japa/assert": "^1.1.0",
    "@japa/expect": "^2.0.0",
    "@japa/runner": "^2.0.9"
  },
  "peerDependenciesMeta": {
    "@japa/assert": {
      "optional": true
    },
    "@japa/expect": {
      "optional": true
    }
  },
  "author": "Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "homepage": "https://github.com/japa/snapshot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/japa/snapshot.git"
  },
  "bugs": {
    "url": "https://github.com/japa/snapshot/issues"
  },
  "keywords": [],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "next",
    "branch": "main",
    "anyBranch": false
  },
  "directories": {
    "test": "tests"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}
