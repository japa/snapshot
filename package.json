{
  "name": "@japa/snapshot",
  "description": "Snapshot testing plugin for Japa",
  "version": "2.0.6",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests",
    "!build/tests_helpers"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "clean": "del-cli build",
    "precompile": "npm run lint",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "release": "release-it",
    "version": "npm run build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "eslint",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels .github/labels.json japa/snapshot",
    "quick:test": "node --import=ts-node-maintained/register/esm bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^2.0.0-beta.6",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@japa/assert": "^3.0.0",
    "@japa/expect": "^3.0.2",
    "@japa/file-system": "^2.3.0",
    "@japa/runner": "^3.1.4",
    "@release-it/conventional-changelog": "^8.0.2",
    "@swc/core": "^1.7.26",
    "@types/dedent": "^0.7.2",
    "@types/node": "^20.16.5",
    "@types/stack-utils": "^2.0.3",
    "c8": "^10.1.2",
    "del-cli": "^5.1.0",
    "eslint": "^9.11.0",
    "expect": "^29.7.0",
    "github-label-sync": "^2.3.1",
    "prettier": "^3.3.3",
    "release-it": "^17.6.0",
    "ts-node-maintained": "^10.9.4",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "dedent": "^1.5.3",
    "jest-message-util": "^29.7.0",
    "magic-string": "^0.30.11",
    "pretty-format": "^29.7.0",
    "stack-utils": "^2.0.6"
  },
  "peerDependencies": {
    "@japa/assert": "^2.0.0 || ^3.0.0",
    "@japa/expect": "^3.0.2",
    "@japa/runner": "^3.1.2"
  },
  "peerDependenciesMeta": {
    "@japa/assert": {
      "optional": true
    },
    "@japa/expect": {
      "optional": true
    }
  },
  "author": "japa, Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "homepage": "https://github.com/japa/snapshot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/japa/snapshot.git"
  },
  "bugs": {
    "url": "https://github.com/japa/snapshot/issues"
  },
  "keywords": [],
  "prettier": "@adonisjs/prettier-config",
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "release-it": {
    "git": {
      "requireCleanWorkingDir": true,
      "requireUpstream": true,
      "commitMessage": "chore(release): ${version}",
      "tagAnnotation": "v${version}",
      "push": true,
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    },
    "npm": {
      "publish": true,
      "skipChecks": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./src/types/main.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
