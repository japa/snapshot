{
  "name": "@japa/snapshot",
  "description": "Snapshot testing plugin for Japa",
  "version": "2.0.0",
  "engines": {
    "node": ">=18.16.0"
  },
  "type": "module",
  "files": [
    "build/src",
    "build/index.d.ts",
    "build/index.js",
    "build/index.d.ts.map"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "clean": "del-cli build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile",
    "release": "np",
    "version": "npm run build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels .github/labels.json japa/snapshot",
    "quick:test": "node --loader ts-node/esm bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.1.6",
    "@adonisjs/prettier-config": "^1.1.6",
    "@adonisjs/tsconfig": "^1.1.6",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@japa/assert": "^2.0.1",
    "@japa/expect": "^3.0.0",
    "@japa/file-system": "^2.0.0",
    "@japa/runner": "^3.0.5",
    "@swc/core": "^1.3.96",
    "@types/dedent": "^0.7.1",
    "@types/node": "^20.8.10",
    "@types/stack-utils": "^2.0.2",
    "c8": "^8.0.0",
    "del-cli": "^5.0.0",
    "eslint": "^8.53.0",
    "expect": "^29.5.0",
    "github-label-sync": "^2.3.1",
    "husky": "^8.0.3",
    "np": "^8.0.4",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "dedent": "^1.5.1",
    "jest-message-util": "^29.5.0",
    "magic-string": "^0.30.5",
    "pretty-format": "^29.5.0",
    "stack-utils": "^2.0.6"
  },
  "peerDependencies": {
    "@japa/assert": "^2.0.0",
    "@japa/expect": "^3.0.0",
    "@japa/runner": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@japa/assert": {
      "optional": true
    },
    "@japa/expect": {
      "optional": true
    }
  },
  "author": "japa, Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "homepage": "https://github.com/japa/snapshot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/japa/snapshot.git"
  },
  "bugs": {
    "url": "https://github.com/japa/snapshot/issues"
  },
  "keywords": [],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "directories": {
    "test": "tests"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  }
}
