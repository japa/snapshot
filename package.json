{
  "name": "@japa/snapshot",
  "description": "Snapshot testing plugin for Japa",
  "version": "2.0.5",
  "engines": {
    "node": ">=18.16.0"
  },
  "main": "build/index.js",
  "type": "module",
  "files": [
    "build",
    "!build/bin",
    "!build/tests",
    "!build/tests_helpers"
  ],
  "exports": {
    ".": "./build/index.js",
    "./types": "./build/src/types/main.js"
  },
  "scripts": {
    "pretest": "npm run lint",
    "test": "c8 npm run quick:test",
    "clean": "del-cli build",
    "precompile": "npm run lint && npm run clean",
    "compile": "tsup-node && tsc --emitDeclarationOnly --declaration",
    "build": "npm run compile",
    "release": "np",
    "version": "npm run build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "lint": "eslint . --ext=.ts",
    "format": "prettier --write .",
    "sync-labels": "github-label-sync --labels .github/labels.json japa/snapshot",
    "quick:test": "node --loader ts-node/esm bin/test.ts"
  },
  "devDependencies": {
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.4.0",
    "@adonisjs/tsconfig": "^1.4.0",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@japa/assert": "^3.0.0",
    "@japa/expect": "^3.0.2",
    "@japa/file-system": "^2.3.0",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.7.26",
    "@types/dedent": "^0.7.2",
    "@types/node": "^20.16.5",
    "@types/stack-utils": "^2.0.3",
    "c8": "^10.1.2",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.1",
    "expect": "^29.7.0",
    "github-label-sync": "^2.3.1",
    "husky": "^9.1.6",
    "np": "^10.0.7",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "dedent": "^1.5.3",
    "jest-message-util": "^29.7.0",
    "magic-string": "^0.30.11",
    "pretty-format": "^29.7.0",
    "stack-utils": "^2.0.6"
  },
  "peerDependencies": {
    "@japa/assert": "^2.0.0 || ^3.0.0",
    "@japa/expect": "^3.0.2",
    "@japa/runner": "^3.1.2"
  },
  "peerDependenciesMeta": {
    "@japa/assert": {
      "optional": true
    },
    "@japa/expect": {
      "optional": true
    }
  },
  "author": "japa, Julien Ripouteau <julien@ripouteau.com>",
  "license": "MIT",
  "homepage": "https://github.com/japa/snapshot#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/japa/snapshot.git"
  },
  "bugs": {
    "url": "https://github.com/japa/snapshot/issues"
  },
  "keywords": [],
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/package"
  },
  "prettier": "@adonisjs/prettier-config",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "np": {
    "message": "chore(release): %s",
    "tag": "latest",
    "branch": "main",
    "anyBranch": false
  },
  "directories": {
    "test": "tests"
  },
  "c8": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "tsup": {
    "entry": [
      "./index.ts",
      "./src/types/main.ts"
    ],
    "outDir": "./build",
    "clean": true,
    "format": "esm",
    "dts": false,
    "sourcemap": true,
    "target": "esnext"
  }
}
